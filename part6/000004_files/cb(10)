(function($) {
	$.format = function(source, params) {
		if (arguments.length == 1) return function() {
			var args = $.makeArray(arguments);
			args.unshift(source);
			return $.format.apply(this, args);
		};
		if (arguments.length > 2 && params.constructor != Array) {
			params = $.makeArray(arguments).slice(1);
		}
		if (params.constructor != Array) {
			params = [params];
		}
		$.each(params, function(i, n) {
			source = source.replace(new RegExp("\\{" + i + "\\}", "g"), n);
		});
		return source;
	};
	$.fn.autocomplete = function(options) {
		var opt = {
			isSpeech : false,
			container : [],
			data : [],
			valueList : [],
			boxList : [],
			type : [],
			extra : true,
			text : null,
			count : 5,
			index : 0,
			input : null,
			window : 'new',
			urlPattern : null,
			displaycode : true
		};
		var typeList = ['fund', 'stock', 'company', 'field', 'concept', 'area', 'hk', 'yyb', 'xxgn', 'usa','F10'];
		var typeInfo = {
			'fund' : {
				name : '\u57FA\u91D1',
				mark : 0
			},
			'stock' : {
				name : '\u80A1\u7968',
				mark : 1
			},
			'company' : {
				name : '\u57FA\u91D1\u516C\u53F8',
				mark : 2
			},
			'field' : {
				name : '\u884C\u4E1A',
				mark : 3
			},
			'concept' : {
				name : '\u6982\u5FF5',
				mark : 4
			},
			'area' : {
				name : '\u5730\u57DF',
				mark : 5
			},
			'hk' : {
				name : '\u6E2F\u80A1',
				mark : 6
			},
			'yyb' : {
				name : '\u8425\u4E1A\u90E8',
				mark : 7
			},
			'yyb' : {
				name : 'Êñ∞ÂÖ¥Ê¶ÇÂøµ',
				mark : 8
			},
			'usa' : {
				name : 'ÁæéËÇ°',
				mark : 9
			},
			'f10' : {
				name : 'f10',
				mark : 10
			}
		};
		var prefix = 'autocomplete';

		var init = function(self) {
			var defaultType = {
				stock: true,
				hk: true,
				fund: true,
				usa:true
			};
			for (var a in options) {
	            if (options[a] != null) {
	            	if (typeInfo[a]) {
	            		defaultType[a] = options[a];
	            	} else {
	            		opt[a] = options[a]
	            	}
	            }
	        }
			for (var a in defaultType) {
	        	if (defaultType[a]) {
	        		opt.type.push(a);
	        	}
	        }
	        opt.input = self;
	        $("#" + prefix + "_" + self.id).remove();
	        opt.container[self.id] = $("<div>").addClass("autocomplete").attr("id", prefix + "_" + self.id).hide().css({"z-index":parseInt(new Date().getTime() / 1000),"position":"fixed"}).appendTo($("body"));
			$(self).attr("autocomplete", "off");
			var $container = opt.container[self.id];
			if (opt.width) {
				$container.width(opt.width);
			} else {
				$container.outerWidth(Math.max($container.width("").outerWidth() + 1, $(self).outerWidth()));
			}
			if (opt.func) {
				$.each(opt.func, function(i, func){
					if (typeof(func) == 'object') {
						func.bindFunc($container, self)
					}
				})
			}
			if (self.webkitSpeech != undefined) {
				opt.isSpeech = true
			}
		};

		var clean = function() {
			var self = opt.input;
			opt.container[self.id].hide();
		};

		var showList = function() {
			var self = opt.input;
			opt.container[self.id].show();
		}
		var setIndex = function(index) {
			var self = opt.input;
			var $div = opt.container[self.id];
			var total = $div.find('dd').size() + $div.find('dt').size();
			if (index < 1 || index > total) {
				return;
			}
			opt.index = index;
			var $target = $div.find(".auto-item[did='" + index + "']");
			$div.find('dd,dt').removeClass('selected');
			$target.addClass('selected');
			$div.show();
		};

		var talog = function() {
			if (opt.statid && window.TA) {
				TA.log({
					id : opt.statid,
					nj : 1,
					_sid : 'img_' + opt.statid
				});
			}
		};

		var redirect = function(url) {
			if (opt.window == 'self') {
				window.location.href = url
			}
			if (opt.window == 'new') {
				window.open(url)
			}
		};

		var submit = function() {
			var text = opt.text,
			encodeText = encodeURIComponent(text),
			self = opt.input,
			index = opt.index,
			defaultText = self.defaultValue,
			$div = opt.container[self.id];
			talog();
			if (!opt.stype || !opt.scode) {
				//Ëß£Êûêindex
				var $target = $div.find(".auto-item[did='" + index + "']");
				if ($target) {
					opt.stype = $target.parent().attr('data-type');
					if ($target[0].nodeName.toUpperCase() == 'DD') {
						opt.scode = $target.attr('data-code');
						opt.spy = $target.attr('data-py');
					}
				}
			}
			if(opt.type.length == 1 && opt.type[0] == 'f10'){
				var $target = $div.find(".auto-item[did='" + index + "']");
				if ($target && !$div.is(':hidden')) {
					$target.trigger('click');
				}else{
					var url = 'http://www.iwencai.com/stockpick/search?typed=1&preParams=&ts=1&f=1&qs=f10stock_query&selfsectsn=&querytype=&searchfilter=&tid=stockpick&w='+encodeText;
					openbrower(url);
				}
				return false;
			}
			if (opt.clickFunc) {
				$.each(opt.clickFunc, function(i, func){
					if (typeof(func) == 'object' && opt.scode) {
						func.run(opt.scode, opt)
					}
				})
				return
			}
			if (text == '' || text == defaultText) {
				redirect('http://search.10jqka.com.cn');
				return
			}
			if (!opt.stype) {
				return
			}
			switch (opt.stype) {
				case 'blog' :
					redirect("http://search.10jqka.com.cn/search?tid=blog&qs=box_ths&w=" + encodeText);
					break;
				case 'forum' :
					redirect("http://search.10jqka.com.cn/search?tid=forum&qs=box_ths&w=" + encodeText);
					break;
				case 'news' :
					redirect("http://search.10jqka.com.cn/search?tid=news&qs=box_ths&w=" + encodeText);
					break;
				case 'stock' :
					if (opt.scode) {
						var url = "http://stockpage.10jqka.com.cn/" + opt.scode + "/";
						if (opt.urlPattern && opt.urlPattern['stock']) {
							url = $.format(opt.urlPattern['stock'], opt.scode)
						}
						redirect(url)
					}
					break;
				case 'fund' :
					if (opt.scode) {
						var url = "http://fund.10jqka.com.cn/" + opt.scode + "/";
						if (opt.urlPattern && opt.urlPattern['fund']) {
							url = $.format(opt.urlPattern['fund'], opt.scode)
						}
						redirect(url)
					}
					break;
				case 'hk' :
					if (opt.scode) {
						var url = "http://stockpage.10jqka.com.cn/HK" + opt.scode.substr(1,4) + "/";
						if (opt.urlPattern && opt.urlPattern['hk']) {
							url = $.format(opt.urlPattern['hk'], opt.scode)
						}
						redirect(url)
					}
					break;
				case 'company' :
					if (opt.scode) {
						var url = "http://fund.10jqka.com.cn/company/" + opt.scode + "/";
						if (opt.urlPattern && opt.urlPattern['company']) {
							url = $.format(opt.urlPattern['company'], opt.scode)
						}
						redirect(url)
					}
					break;
				case 'field' :
					if (opt.spy) {
						var url = "http://q.10jqka.com.cn/stock/thshy/" + opt.spy + "/";
						if (opt.urlPattern && opt.urlPattern['field']) {
							url = $.format(opt.urlPattern['field'], opt.spy)
						}
						redirect(url)
					}
					break;
				case 'concept' :
					if (opt.spy) {
						var url = "http://q.10jqka.com.cn/stock/gn/" + opt.spy + "/";
						if (opt.urlPattern && opt.urlPattern['concept']) {
							url = $.format(opt.urlPattern['concept'], opt.spy)
						}
						redirect(url)
					}
					break;
				case 'area' :
					if (opt.spy) {
						var url = "http://q.10jqka.com.cn/stock/dy/" + opt.spy + "/";
						if (opt.urlPattern && opt.urlPattern['area']) {
							url = $.format(opt.urlPattern['area'], opt.spy)
						}
						redirect(url)
					}
					break;
				case 'usa' :
					if (opt.scode) {
						var url = "http://stockpage.10jqka.com.cn/" + opt.scode + "/";
						if (opt.urlPattern && opt.urlPattern['usa']) {
							url = $.format(opt.urlPattern['usa'], opt.scode)
						}
						redirect(url)
					}
					break;
				default:
					break;
			}
			opt.stype = null;
			opt.scode = null;
			opt.spy = null;
		};

		var enter = function () {
			var self = opt.input,
				$div = opt.container[self.id],
				$target = $div.find('dd.selected').eq(0);
			opt.stype = $target.parent().attr('data-type');
			opt.scode = null;
			opt.spy = null;
			if ($target[0].nodeName.toUpperCase() == 'DD') {
				opt.scode = $target.attr('data-code');
				opt.spy = $target.attr('data-py');
			}
			submit()
		};

		var bindEvent = function() {
			var self = opt.input,
				$div = opt.container[self.id];
			$div.find('.auto-item').each(function(index, el) {
				$(this).attr('did', index + 1);
			});
			$div.find('dd').each(function(index, el) {
				$(this).mouseover(function(event) {
					$div.find('dd,dt').removeClass('selected');
					$(this).addClass('selected')
				});
			});
			$div.find('dt').each(function(index, el) {
				$(this).mouseover(function(event) {
					$div.find('dd,dt').removeClass('selected');
					$(this).addClass('selected')
				});
			});
			if(opt.type.length == 1 && opt.type[0] == 'f10'){
				return false;
			}
			$div.find('dd,dt').each(function(index, el) {
				$(this).click(function(event) {
					opt.stype = $(event.target).parent().attr('data-type');
					opt.scode = null;
					opt.spy = null;
					if (event.target.nodeName.toUpperCase() == 'DD') {
						opt.scode = $(event.target).attr('data-code');
						opt.spy = $(event.target).attr('data-py');
					}
					submit()
				});
			});
		};

		var render = function(data) {
			var self = opt.input,
				type = opt.type,
				$div = opt.container[self.id],
				$box = [];
			for (var i = 0; i < type.length; i++) {
				$box[type[i]] = $("<dl data-type='" + type[i] + "'><dt class='auto-item'>\u641C\"<strong></strong>\"\u76F8\u5173" + typeInfo[type[i]].name + "</dt></dl>");
			}
			$.each(data, function(keyData, items) {
				if (items instanceof Array) {
					opt.data[keyData] = [];
					$.each(items, function(key, item) {
						opt.data[keyData][key] = item.split("||");
						var codeInfo = opt.data[keyData][key][1].split(" "),
							mark = opt.data[keyData][key][0];
						if (typeof (codeInfo[3]) != 'undefined') {
							$("<dd class='auto-item' data-code='" + codeInfo[0] + "' data-py='" + codeInfo[2] + "'></dd>").append("<span>" + (opt.displaycode ? codeInfo[0] : '') + "</span>" + codeInfo[1]).appendTo($box[typeList[mark]]);
						} else {
							$("<dd class='auto-item' data-code='" + codeInfo[0] + "'></dd>").append("<span>" + (opt.displaycode ? codeInfo[0] : '') + "</span>" + codeInfo[1]).appendTo($box[typeList[mark]]);
						}
					});
				} else {
					opt.data[keyData] = items.split("||");
					var codeInfo = opt.data[keyData][1].split(" "),
						mark = opt.data[keyData][0];
					if (typeof (codeInfo[3]) != 'undefined') {
						$("<dd class='auto-item' data-code='" + codeInfo[0] + "' data-py='" + codeInfo[2] + "'></dd>").append("<span>" + (opt.displaycode ? codeInfo[0] : '') + "</span>" + codeInfo[1]).appendTo($box[typeList[mark]]);
					} else {
						$("<dd class='auto-item' data-code='" + codeInfo[0] + "'></dd>").append("<span>" + (opt.displaycode ? codeInfo[0] : '') + "</span>" + codeInfo[1]).appendTo($box[typeList[mark]]);
					}
				}
			});


			$div.empty();
			for (a in $box) {
				$div.append($box[a])
			}
			if (opt.extra) {
				$("<dl data-type='news'><dt class='auto-item'>\u641C\"<strong></strong>\"\u76F8\u5173\u65B0\u95FB&gt;&gt;</dt></dl>").appendTo($div);
				$("<dl data-type='forum'><dt class='auto-item'>\u641C\"<strong></strong>\"\u76F8\u5173\u8BBA\u575B&gt;&gt;</dt></dl>").appendTo($div);
				$("<dl data-type='blog'><dt class='auto-item'>\u641C\"<strong></strong>\"\u76F8\u5173\u535A\u5BA2&gt;&gt;</dt></dl>").appendTo($div)
			}
			$div.find('strong').each(function() {
				$(this).html(opt.text)
			});
			bindEvent();
			setIndex(2);
		};

		var show = function() {
			var self = opt.input;
			var newtype = opt.type;
			opt.data = [];
			if (opt.isSpeech) {
				opt.text = self.value
			}
			if (opt.valueList[opt.text]) {
				render(opt.valueList[opt.text])
			} else {
				if (opt.type == 1) {
					newtype = 0;//Á±ªÂûã‰∏∫1ÁöÑÊó∂ÂÄôÔºåÂΩìÂÅö0ÂÖ®ÈáèËé∑ÂèñÊï∞ÊçÆ
				}
				/*if(opt.type.length == 1 && opt.type[0] == 'f10'){
					showRecommendQuery();
					return false;
				}*/
				$.ajax({
					url: 'http://news.10jqka.com.cn/public/index_keyboard_' + encodeURIComponent(opt.text) + '_' + newtype + '_' + opt.count + '_jsonp.html',
					type: 'GET',
					dataType: 'jsonp',
					jsonpCallback: 'jsonp',
					jsonp: false,
					cache: true
				}).done(function(json) {
					opt.valueList[opt.text] = json;
					render(json)
				})
			}
		};

		var showRecommendQuery = function (showtype){
			clean();
			var txt = '';
			if(encodeURIComponent(opt.text) != 'null'){
				txt = encodeURIComponent(opt.text);
			}
			if (txt == '') {
				txt = 'isnulltxt';
			}
			sendTalog('f10_stock_searchinput_click');
			$.ajax({
				url: 'http://basic.10jqka.com.cn/api/stock/searchForWord/' + txt + '/' + $('#stockCode').val()+'/no/',
				type: 'GET',
				dataType: 'json',
				cache: true,
				timeout : 500, //Ë∂ÖÊó∂Êó∂Èó¥ËÆæÁΩÆÔºåÂçï‰ΩçÊØ´Áßí
				success: function(jsonp){
					var dataList = jsonp;
					var self = opt.input,
						type = opt.type,
						$div = opt.container[self.id],
						$box = $('<dl data-type="f10"></dl>');
					if(showtype == 'default'){//ÈªòËÆ§ËØ≠Âè•
						for(var i = 0;i < dataList.length; i++){
							var dataUrl = dataList[i].url;
							if(dataList[i].type == 'f10') {
								var infoArr = dataUrl.split('|');
								var code = infoArr[0];
								var block = infoArr[1];
								dataUrl = 'http://basic.10jqka.com.cn/' + code + '/' + block;
								if(i < 3){
									$('<dd did="' + (i+1) + '" jumptype="f10" code="' + code +'" block="' + block +'" class="auto-item" data-url="' + dataUrl + '" data-type="f10"></dd>').append('<i class="icona">' + (i+1) + '</i><span>' + dataList[i].name + '</span>').appendTo($box);
								}else{
									$('<dd did="' + (i+1) + '" jumptype="f10"code="' + code +'" block="' + block +'" class="auto-item" data-url="' + dataUrl + '" data-type="f10"></dd>').append('<i class="iconb">' + (i+1) + '</i><span>' + dataList[i].name + '</span>').appendTo($box);
								}
							}else{
								if(i < 3){
									$('<dd did="' + (i+1) + '" jumptype="iwc" class="auto-item" data-url="' + dataUrl + '" data-type=""></dd>').append('<i class="icona">' + (i+1) + '</i><span>' + dataList[i].name + '</span>').appendTo($box);
								}else{
									$('<dd did="' + (i+1) + '" jumptype="iwc" class="auto-item" data-url="' + dataUrl + '" data-type=""></dd>').append('<i class="iconb">' + (i+1) + '</i><span>' + dataList[i].name + '</span>').appendTo($box);
								}
							}
						}
					}else if(getCookie('searchHistory') && self.value.length == 0){//Áî®Êà∑ÊêúÁ¥¢ÂéÜÂè≤
						var temp = getCookie('searchHistory');
						if(temp != ''){
							var historyArr = JSON.parse(getCookie('searchHistory'));
							for(var i = 0;i < historyArr.length; i++){
								var dataUrl = historyArr[i].url;
								var code = historyArr[i].code;
								var block = historyArr[i].block;
								var title = historyArr[i].title;
								var jumptype = historyArr[i].jumptype;
								if(jumptype == 'f10'){
									if(i < 3){
										$('<dd did="' + (i+1) + '" jumptype="f10" code="' + code +'" block="' + block +'" class="auto-item" data-url="' + dataUrl + '" data-type="f10"></dd>').append('<i class="icona">' + (i+1) + '</i><span>' + title + '</span>').appendTo($box);
									}else{
										$('<dd did="' + (i+1) + '" jumptype="f10" code="' + code +'" block="' + block +'" class="auto-item" data-url="' + dataUrl + '" data-type="f10"></dd>').append('<i class="iconb">' + (i+1) + '</i><span>' + title + '</span>').appendTo($box);
									}
								}else{
									if(i < 3){
										$('<dd did="' + (i+1) + '" jumptype="iwc" code="' + code +'" block="' + block +'" class="auto-item" data-url="' + dataUrl + '" data-type=""></dd>').append('<i class="icona">' + (i+1) + '</i><span>' + title + '</span>').appendTo($box);
									}else{
										$('<dd did="' + (i+1) + '" jumptype="iwc" code="' + code +'" block="' + block +'" class="auto-item" data-url="' + dataUrl + '" data-type=""></dd>').append('<i class="iconb">' + (i+1) + '</i><span>' + title + '</span>').appendTo($box);
									}
								}

							}
							$('<p style="height: 20px;"><a class="clean-txt" href="###">\u6e05\u9664\u8bb0\u5f55</a></p>').appendTo($box);
							setTimeout(function(){
								$('#autocomplete_search_input .clean-txt').one('click',function(){
									cleanHistory();
								});
							},200);
						}
					}else if (self.value.length > 0) {//ÊåâÁî®Êà∑ËæìÂÖ•ÊòæÁ§∫ËØ≠Âè•
						$box = $('<dl data-type="f10" class="sty"></dl>');
						for(var i = 0;i < dataList.length; i++){
							var userValue = self.value;
							var dataValue = dataList[i].name;
							var dataUrl = dataList[i].url;
							if(dataValue.indexOf(userValue) > -1){
								dataValue = dataValue.replace(userValue,'<span class="ylw">' + userValue + '</span>');
							}
							if(dataList[i].type == 'f10') {
								var infoArr = dataUrl.split('|');
								var code = infoArr[0];
								var block = infoArr[1];
								dataUrl = 'http://basic.10jqka.com.cn/' + code + '/' + block;
								if(i < 3){
									$('<dd did="' + (i+1) + '" jumptype="f10" code="' + code +'" block="' + block +'" class="auto-item" data-url="' + dataUrl + '" data-type="f10"></dd>').append('<span>' + dataValue + '</span>').appendTo($box);
								}else{
									$('<dd did="' + (i+1) + '" jumptype="f10" code="' + code +'" block="' + block +'" class="auto-item" data-url="' + dataUrl + '" data-type="f10"></dd>').append('<span>' + dataValue + '</span>').appendTo($box);
								}
							}else{
								if(i < 3){
									$('<dd did="' + (i+1) + '" jumptype="iwc" class="auto-item" data-url="' + dataUrl + '" data-type=""></dd>').append('<span>' + dataValue + '</span>').appendTo($box);
								}else{
									$('<dd did="' + (i+1) + '" jumptype="iwc" class="auto-item" data-url="' + dataUrl + '" data-type=""></dd>').append('<span>' + dataValue + '</span>').appendTo($box);
								}
							}
						};
					}
					opt.text = '';
					$div.empty();
					$div.append($('<div class="bd"></div>').append($box));
					$div.append($('<div class="ft"></div>'));
					//showList();

					bindEvent();
					setIndex(1);
				},
				complete : function(XMLHttpRequest,status) { //ËØ∑Ê±ÇÂÆåÊàêÂêéÊúÄÁªàÊâßË°åÂèÇÊï∞
					if(status=='timeout') {//Ë∂ÖÊó∂,statusËøòÊúâsuccess,errorÁ≠âÂÄºÁöÑÊÉÖÂÜµ
						if($('#f10ajax').length < 1){
							$('body').append($('<input id="f10ajax"  type="hidden" value="empty">'));
							showRecommendQuery(showtype);
						}
					}
				}
			});

		}

		this.each(function() {
			var self = this,
				requestTimer = 0,
				focus = false;
			init(self);
			if (opt.isSpeech) {
				self.onwebkitspeechchange = function() {
					show()
				}
			}
			$(self).focus(function() {
				//Âè™ÂÆö‰Ωç‰∏ÄÊ¨°abccccccccccccc
				//if (!focus) {
					/*opt.container[self.id].position($.extend({
						of: $(self)
					}, {
						my: "left top",
						at: "left bottom",
						collision: "none"
					}));*/
					var _wp = $(".wrapper").outerWidth();
					var _wd = $(window).width();
					var _icon = $(".tips-icon").outerWidth()+4;
					var _left = parseFloat($(self).offset().left);
					$(opt.container[self.id]).css({
						left:_left,
						display:"block",
						top:"158px"
					});
				//}
				focus = true;
				if(focus){
					if(opt.type.length == 1 && opt.type[0] == 'f10'){
						
						if(self.value == self.defaultValue || ($("#defaultcontent").length>0 && self.value == $("#defaultcontent").val())){
							showRecommendQuery('default');
						}else{
							showRecommendQuery();
						}
						/*if(true){
							showRecommendQuery('default');
						}*/
						self.value = '';
					}
				}

			}).keyup(function(event) {
				opt.text = self.value;
				var length = opt.text.length,
					keycode = event.keyCode,
					index = opt.index;
				if (keycode > 40 || keycode == 8 || keycode == 32) {
					if(opt.type.length == 1 && opt.type[0] == 'f10'){
						if (length > 0 && length < 20) {
							if (requestTimer == 0) {
								//Âª∂Ëøü100ÊØ´ÁßíËØ∑Ê±ÇÊé•Âè£abccccccccccccccc
								requestTimer = setTimeout(function() {
									showRecommendQuery();
								}, 100)
							}
						} else {
							clean()
						}
					}else {
						if (length > 0 && length < 8) {
							if (requestTimer == 0) {
								//Âª∂Ëøü100ÊØ´ÁßíËØ∑Ê±ÇÊé•Âè£abccccccccccccccc
								requestTimer = setTimeout(function() {
									show()
								}, 100)
							}
						} else {
							clean()
						}
					}
				} else if (index !== null) {
					if (keycode == 38) {
						setIndex(index - 1)
					} else if (keycode == 40) {
						setIndex(index + 1)
					} else if (keycode == 27) {
						setIndex(0)
					} else if (keycode == 13) {
						enter()
					}
				}
				event.preventDefault()
			}).keydown(function(event) {
				clearTimeout(requestTimer);
				requestTimer = 0
			}).blur(function(event) {
				setTimeout(function() {
					clean()
				}, 200)
			});
		});

	}
})(jQuery);
//@charset "gbk";
function setCookie(name, value, time) {
    var strsec = getsec(time);
    var exp = new Date();
    exp.setTime(exp.getTime() + strsec * 1);
    document.cookie = name + '=' + escape(value) + ';expires=' + exp.toGMTString() + ';path=/;domain=.10jqka.com.cn';
}

function getsec(str) {
    var str1 = str.substring(1, str.length) * 1;
    var str2 = str.substring(0, 1);
    if (str2 == 's') {
        return str1 * 1000;
    } else if (str2 == 'h') {
        return str1 * 60 * 60 * 1000;
    } else if (str2 == 'd') {
        return str1 * 24 * 60 * 60 * 1000;
    }
}

function getCookie(name) {
    var arr, reg = new RegExp('(^| )' + name + '=([^;]*)(;|$)');
    if (arr = document.cookie.match(reg)) {
        return unescape(arr[2]);
    } else {
        return null;
    }
}

function delCookie(name) {
    var exp = new Date();
    exp.setTime(exp.getTime() - 1);
    var cval = getCookie(name);
    if (cval != null) {
        document.cookie = name + "=" + cval + ";expires=" + exp.toGMTString() + ';path=/;domain=.10jqka.com.cn';;
    }
}

function searchAll($btn) {
    this.bindFunc = function($auto, input) {
        $btn.unbind('click').bind('click', function() {
            var url = '',
                code = $auto.find('dd.selected').eq(0).attr('data-code'),
                type = $auto.find('dd.selected').eq(0).parent().attr('data-type');
            if (typeof(code) != 'undefined') {
                if (type == 'stock') {
                    url = 'http://stockpage.10jqka.com.cn/' + code + '/';
                }
                if (type == 'fund') {
                    url = 'http://fund.10jqka.com.cn/' + code + '/';
                }
            } else {
                if (type == 'f10') {
                    if (input.value != input.defaultValue) {
                        var $dd = $auto.find('dd.selected').eq(0);
                        if (!$auto.is(':hidden') && $dd) {
                            $dd.trigger('click');
                        } else { //¡Ìøº¬«Œﬁ∂‘”¶—°œÓµƒ«Èøˆ
                            url = 'http://www.iwencai.com/stockpick/search?typed=1&preParams=&ts=1&f=1&qs=f10stock_query&selfsectsn=&querytype=&searchfilter=&tid=stockpick&w=' + encodeURIComponent(input.value);
                        }
                    } else {
                        url = 'http://www.iwencai.com/stockpick?tid=stockpick&qs=f10stock_query';
                    }

                } else {
                    if (input.value != input.defaultValue) {
                        url = 'http://www.iwencai.com/stockpick/search?typed=1&preParams=&ts=1&f=1&qs=f10stock_query&selfsectsn=&querytype=&searchfilter=&tid=stockpick&w=' + encodeURIComponent(input.value);
                    } else {
                        url = 'http://www.iwencai.com/stockpick?tid=stockpick&qs=f10stock_query';
                    }
                }
            }

            if (type != 'f10' && !input.value) {
                url = 'http://www.iwencai.com/stockpick?tid=stockpick&qs=f10stock_query';
            }
            if (url) {
                openbrower(url, $btn);
                //window.open(url);
            }
        })
    }
};

function searchLog(word, title) {
    var image = document.createElement("img");
    image.style.display = "none";
    document.body.appendChild(image);
    image.src = "http://basic.10jqka.com.cn/api/stock/saveSearchWord.php?word=" + word + "&click=" + title;
}
$('#autocomplete_search_input dd').live('click', function() {
    var url = $(this).attr('data-url');
    var obj = this;
    var type = $(this).attr('data-type');
    openbrower(url, obj, type);
});

function openbrower(url, obj, type, e) {
    if (type == 'f10') {
        sendTalog('f10_stock_search_gotof10#f10_stock_search_goto');
    } else {
        sendTalog('f10_stock_search_goiwc#f10_stock_search_goto');
    }
    var urls = url;
    var obj = $(obj);
    var cookies = JSON.parse(getCookie('searchHistory'));
    if (cookies) {
        if ($('#search_input').val() == cookies[0]['title']) {
            obj.attr('block', cookies[0]['block']);
            obj.attr('code', cookies[0]['code']);
            //obj.attr('jumptype', cookies[0]['jumptype']);
            type = cookies[0]['jumptype'];
        }
    }
    if ($('#search_input').val() == $("#defaultcontent").val()) {
        if ($("#locationttype").val() == 1) {
            obj.attr('block', $("#location").val());
            obj.attr('code', $('#stockCode').val());
            type = 'f10';
        }
    }
    var code = obj.attr('code') || '';
    var block = obj.attr('block') || '';
    var title = obj.children('span').text() || $('#search_input').val();
    var jumptype = obj.attr('jumptype') || '';
    var historyArr = []; //À—À˜¿˙ ∑º«¬º
    var thisInfo, temp, len;

    if ($('#search_input').val() || title) {
        searchLog($('#search_input').val(), title);
    } else {
        sendTalog('f10_stock_search_empty');
    }
    if (!getCookie('searchHistory')) {
        setCookie('searchHistory', '', 'h24');
    }
    thisInfo = {
        'url': url,
        'code': code,
        'block': block,
        'title': title,
        'jumptype': jumptype
    };
    temp = getCookie('searchHistory');
    if (!!temp) {
        historyArr = JSON.parse(getCookie('searchHistory'));
    }
    //∂˛¥ŒÀ—À˜∏ƒ–¥µÿ÷∑
    for (var i = 0; i < historyArr.length; i++) {
        if (historyArr[i].title == $('#search_input').val()) {
            thisInfo.url = url = historyArr[i].url;
            if (url.indexOf('http://basic.10jqka.com.cn') > -1) {
                type = 'f10';
            }
            break;
        }
    }
    for (var i = 0; i < historyArr.length; i++) {
        if (historyArr[i].title == thisInfo.title) {
            var arr1 = historyArr.slice(0, i);
            var arr2 = historyArr.slice(i + 1, historyArr.length);
            historyArr = arr1.concat(arr2);
            break;
        }
    }
    historyArr.unshift(thisInfo);
    len = historyArr.length;
    if (len > 8) {
        historyArr = historyArr.slice(0, 8);
    }
    temp = JSON.stringify(historyArr);
    setCookie('searchHistory', temp, 'h24');
    $('#search_input').val(title);

    if (urls.indexOf('http://basic.10jqka.com.cn') > -1) {
        type = 'f10';
    }
    if (type == 'f10') {
        try {
            if (code == $('#stockCode').val()) {
                var nowurl = window.location.href;
                var cates = block.split('#');
                if (cates[0] && cates[1] && nowurl.indexOf(cates[0]) > 0) {
                    $("a[name='" + block + "']").eq(0).click();
                } else {
                    window.location.href = "http://basic.10jqka.com.cn/" + code + '/' + block;
                }
            } else {
                external.createObject('Util');
                setReviewJumpState(escape(block));
                window.API.use({
                    method: 'Quote.switchPage',
                    data: {
                        code: code
                    },
                    success: function() {

                    }

                })
            }
        } catch (e) {
            window.open(url);
        }
    } else {
        try {
            external.createObject('Util');
            window.API.use({
                method: 'Util.openURL',
                data: [urls, 1]
            })
        } catch (e) {
            window.open(url);
        }
    }
    return false;
}

function cleanHistory() {
    delCookie('searchHistory');
    $('#search_input').val($('#search_input').get(0).defaultValue);
}
if (getCookie('searchHistory')) {
    var historyArr = JSON.parse(getCookie('searchHistory'));
    var title = historyArr[0].title;
    $('#search_input').val(title);
}
if (!getCookie('searchGuide')) {
    //$('.tips-box').show();
    setCookie('searchGuide', 'sg', 'd365');
}
