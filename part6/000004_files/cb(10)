$("#search_input").focus(function() {
    $(this).closest('div').addClass('focused');
}).blur(function() {
    $(this).closest('div').removeClass('focused');
});
$(document).click(function(e) {
    if (!$(e.target).is('#search_input') && !$(e.target).is('#search_submit')) {
        if (getCookie('searchHistory')) {
            var historyArr = JSON.parse(getCookie('searchHistory'));
            var title = historyArr[0].title;
            $('#search_input').val(title);
        } else {
            $('#search_input').val($('#search_input').get(0).defaultValue);
        }
    }
});
$.fn.autocomplete && $("#search_input").autocomplete({
    stock: false,
    hk: false,
    fund: false,
    extra: false,
    usa: false,
    f10: true,
    func: [new searchAll($("#search_submit"))]
});

$(window).scroll(function() {
    if (!$("#autocomplete_search_input").is(':hidden')) {
        $("#search_input").blur();
        $("#autocomplete_search_input").hide();
    }
})

function changeDefaultWord() {
    $.ajax({
        url: 'http://basic.10jqka.com.cn/xml/defaultWord.json',
        type: 'GET',
        dataType: 'json',
        cache: true,
        success: function(jsonp) {
            if (!jsonp) {
                return false;
            }
            var obj = jsonp;
            if (obj.length > 0) {
                var index = Math.floor((Math.random() * obj.length));
                if (!getCookie('searchHistory')) {
                    $("#search_input").val(obj[index]['word']);
                }
                var html = "<input type='hidden' id='locationttype' value='" + obj[index]['locationtype'] + "'/>";
                var html = html + "<input type='hidden' id='location' value='" + obj[index]['location'] + "'/>";
                var html = html + "<input type='hidden' id='defaultcontent' value='" + obj[index]['word'] + "'/>";
                $("#wordraddom").html(html);
            }
        }
    });
}
changeDefaultWord();
$(function() {
    function initFloatBox() {
        var _searchbar = $(".iwc_searchbar");
        var _wp = $(".wrapper").outerWidth();
        var _wd = $(window).width();
        var _left;
        if (_wd > _wp) {
            _left = (_wd - _wp) / 2 + _wp - _searchbar.width();
        } else {
            _left = _wd - _searchbar.width();
        }
        _searchbar.css({
            left: _left
        }).show();
        $('#autocomplete_search_input').hide();
    }
	var hashs = window.location.hash.substring(1);
    hashArrays = hashs.split('='); 
    if (location.href.indexOf('interactive.html') > -1 || hashArrays[0] == 'stockpage') {
        $('.iwc_searchbar').hide();
    } else {
        var _searchbar = $(".iwc_searchbar");
        if (_searchbar.length > 0) {
            initFloatBox();
            _searchbar.click(function() { $(this).removeClass("float_box") });

            var resizeTimer = null;
            $(window).resize(function() {
                if (resizeTimer) {
                    clearTimeout(resizeTimer)
                }
                resizeTimer = setTimeout(function() {
                    initFloatBox();
                }, 400);
            });
        }
    }
});
var _searchbar = $(".iwc_searchbar");
$(window).scroll(function() {
    _searchbar.addClass("float_box");
    var _scrolltop = $(window).scrollTop();
    if (_scrolltop == 0) {
        _searchbar.removeClass("float_box");
    }
});

//20160108 À—À˜øÚ«¯”ÚÕ‚3√Îœ˚ ß
function hideEvent() {
    setTimeout(function() {
        if ($('.iwc_searchbar').attr('mouseTag') == 'leave') {
            $('body').trigger('click');
            $('#search_input').trigger('blur');
        }
        $(document).bind('mousemove', function(e) {
            bodyMousemoveEvent(e);
        });
    }, 3000);
}

function getPostion(e) {
    e = e || window.event;
    var x = e.pageX;
    var y = e.pageY;
    return [x, y];
}
var hideCount = 0;

function bodyMousemoveEvent(e) {
    hideCount = 0;
    if ($('.iwc_searchbar').hasClass('focused')) {
        setTimeout(function() {
            var minX = $('.iwc_searchbar').offset().left,
                maxX = $('.iwc_searchbar').offset().left + $('#search_input').outerWidth() + $('#search_submit').outerWidth(),
                minY = $('.iwc_searchbar').offset().top,
                maxY = $('.iwc_searchbar').offset().top + $('.iwc_searchbar').outerHeight() + $('#autocomplete_search_input').outerHeight(),
                pos = getPostion(e);
            if (!(pos[0] >= minX && pos[0] <= maxX && pos[1] >= minY && pos[1] <= maxY)) {
                hideCount++;
                if (hideCount == 1) { //±‹√‚hideEvent÷¥––∂‡¥Œ
                    $(document).unbind('mousemove');
                    hideEvent();
                }
            }
        }, 500);
    }
}
$('.iwc_searchbar,#autocomplete_search_input').mouseenter(function() {
    $('.iwc_searchbar').attr('mouseTag', 'enter');
}).mouseleave(function() {
    $('.iwc_searchbar').attr('mouseTag', 'leave');
});
$(document).bind('mousemove', function(e) {
    bodyMousemoveEvent(e);
});//@charset "utf-8";
$('.tips-box a').click(function(e){
    if($(this).hasClass('next')){
        var index = $(this).parents('.info').index() + 1;
    }else{
        var index = $(this).parents('.info').index() - 1;
    }
    if(index >= 0){
        $('.tips-box .info').hide().eq(index).show();
    }
});
$('.tips-box .close').click(function(){
    $('.tips-box').hide();
});
$('.tips-icon').click(function(){
    $('.tips-box').show();
});
$(document).click(function(e){
    if($(e.target).closest('.tips-box').length < 1 && $(e.target).closest('.tips-icon').length < 1){
        $('.tips-box').hide();
    }
})
